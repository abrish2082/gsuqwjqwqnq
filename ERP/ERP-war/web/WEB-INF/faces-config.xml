        <managed-bean-class>et.gov.eep.hrms.controller.payroll.BackPayment</managed-bean-class>
        <managed-bean-scope>view</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>thirdPartyInfoRegistrationController</managed-bean-name>
        <managed-bean-class>et.gov.eep.hrms.controller.payroll.ThirdPartyInfoRegistrationController</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>thirdPartyRegistration</managed-bean-name>
        <managed-bean-class>et.gov.eep.hrms.controller.payroll.ThirdPartyRegistration</managed-bean-class>
        <managed-bean-scope>view</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>overtmeCalculationController</managed-bean-name>
        <managed-bean-class>et.gov.eep.hrms.controller.overtimeCalc.OvertmeCalculationController</managed-bean-class>
        <managed-bean-name>fixedAssetReturnController</managed-bean-name>
        <managed-bean-class>et.gov.eep.mms.controller.FixedAssetReturnController</managed-bean-class>
        <managed-bean-scope>view</managed-bean-scope>
    </managed-bean>
    <managed-bean>
