        <managed-bean-class>et.gov.eep.hrms.controller.recruitment.HrCandidatController</managed-bean-class>
        <managed-bean-scope>view</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>postingController</managed-bean-name>
        <managed-bean-class>et.gov.eep.fcms.controller.PostingController</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
