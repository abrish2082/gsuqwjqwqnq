        <managed-bean-class>et.gov.eep.hrms.controller.termination.CompensationController</managed-bean-class>
        <managed-bean-scope>view</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>FixedAssetRegistrationController</managed-bean-name>
        <managed-bean-class>et.gov.eep.ifrs.FixedAssetRegistrationController</managed-bean-class> 
        <managed-bean-scope>view</managed-bean-scope>
    </managed-bean>    
    <navigation-rule>
        <from-view-id>/erp/fcms/Loan/principalpayschedule.xhtml</from-view-id>
        <navigation-case>
            <from-action>#{principalPayScheduleController.navigateToLoanPage}</from-action>
            <from-outcome>success</from-outcome>
            <to-view-id>/erp/fcms/Loan/loanagreementreg.xhtml</to-view-id>
            <redirect/>        
        </navigation-case>
    </navigation-rule>
    <navigation-rule> 
        <from-view-id>/erp/mms/ManageFixedAsset/Insurance.xhtml</from-view-id>
        <navigation-case>
            <from-action>#{insuranceController.navigateToMmsTransmission}</from-action>
            <from-outcome>success</from-outcome>
            <to-view-id>/erp/mms/ManageFixedAsset/FixedAssetTransmission.xhtml</to-view-id>
            <redirect/>        
        </navigation-case>
    </navigation-rule>
    <navigation-rule> 
        <from-view-id>/erp/mms/ManageFixedAsset/Insurance.xhtml</from-view-id>
        <navigation-case>
            <from-action>#{insuranceController.navigateToMmsSubstaion}</from-action>
            <from-outcome>success</from-outcome>
            <to-view-id>/erp/mms/ManageFixedAsset/FixedAssetSubstation.xhtml</to-view-id>
            <redirect/>        
        </navigation-case>
    </navigation-rule>
    
    <navigation-rule> 
        <from-view-id>/erp/mms/ManageFixedAsset/Insurance.xhtml</from-view-id>
        <navigation-case>
            <from-action>#{insuranceController.navigateToMmsGeothermal}</from-action>
            <from-outcome>success</from-outcome>
            <to-view-id>/erp/mms/ManageFixedAsset/Geothermal.xhtml</to-view-id>
            <redirect/>        
        </navigation-case>
    </navigation-rule>
    <navigation-rule> 
        <from-view-id>/erp/mms/ManageFixedAsset/Insurance.xhtml</from-view-id>
        <navigation-case>
            <from-action>#{insuranceController.navigateToMmsHpower}</from-action>
            <from-outcome>success</from-outcome>
            <to-view-id>/erp/mms/ManageFixedAsset/FixedAssetHydropower.xhtml</to-view-id>
            <redirect/>        
        </navigation-case>
    </navigation-rule>
    <navigation-rule> 
        <from-view-id>/erp/mms/ManageFixedAsset/Insurance.xhtml</from-view-id>
        <navigation-case>
            <from-action>#{insuranceController.navigateToMmsTransport}</from-action>
            <from-outcome>success</from-outcome>
            <to-view-id>/erp/mms/ManageFixedAsset/transport.xhtml</to-view-id>
            <redirect/>        
        </navigation-case>
    </navigation-rule>
    <navigation-rule> 
        <from-view-id>/erp/mms/ManageFixedAsset/Insurance.xhtml</from-view-id>
        <navigation-case>
            <from-action>#{insuranceController.navigateToMmsWind}</from-action>
            <from-outcome>success</from-outcome>
            <to-view-id>/erp/mms/ManageFixedAsset/wind.xhtml</to-view-id>
            <redirect/>        
        </navigation-case>
    </navigation-rule>
    <navigation-rule> 
        <from-view-id>/erp/mms/ManageFixedAsset/Insurance.xhtml</from-view-id>
        <navigation-case>
            <from-action>#{insuranceController.navigateToMmsBldg}</from-action>
            <from-outcome>success</from-outcome>
            <to-view-id>/erp/mms/ManageFixedAsset/building.xhtml</to-view-id>
            <redirect/>        
        </navigation-case>
    </navigation-rule>
    <navigation-rule> 
        <navigation-case>
            <from-outcome>LoggedIn</from-outcome>
            <to-view-id>/LoggedIn.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>
        
    <navigation-rule>
        <from-view-id>/erp/hrms/discipline/disciplineRequest.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>offenceType</from-outcome>
            <to-view-id>/erp/hrms/discipline/displineOffenseType.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>
     
</faces-config>
