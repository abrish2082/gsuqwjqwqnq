<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.2"
              xmlns="http://xmlns.jcp.org/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">
    <application>
        <locale-config>
            <default-locale>am</default-locale>
            <supported-locale>en</supported-locale>       
        </locale-config>
        <resource-bundle>
            <base-name>/et/gov/eep/hrms/en</base-name>
            <var>HRMS</var>
        </resource-bundle>
        <resource-bundle>
            <base-name>/et/gov/eep/mms/en</base-name>
            <var>MMS</var>
        </resource-bundle>
        <resource-bundle>
            <base-name>/et/gov/eep/prms/en</base-name>
            <var>PRMS</var>
        </resource-bundle>
        <resource-bundle>
            <base-name>/et/gov/eep/fcms/en</base-name>
            <var>FCMS</var>
        </resource-bundle>
        <resource-bundle>
            <base-name>seBundle</base-name>
            <var>secBundle</var>
        </resource-bundle>
    </application>
    <converter>
        <converter-id>dateConverter</converter-id>
        <converter-class>et.gov.eep.commonApplications.utility.DateConverter</converter-class>
    </converter>
    <configuration>
        <argLine>-Xmx1024mm -XX:MaxPermSize=1024m</argLine>
    </configuration>
    <managed-bean>
