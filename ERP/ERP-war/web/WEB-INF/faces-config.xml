        <managed-bean-name>hrRecruitmentRequestController</managed-bean-name>
        <managed-bean-class>et.gov.eep.hrms.controller.recruitment.HrRecruitmentRequestController</managed-bean-class>
        <managed-bean-scope>view</managed-bean-scope>
    </managed-bean>
    
    <managed-bean>
